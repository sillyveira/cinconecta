{
  "dependencies": {
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "mongoose": "^8.9.5",
    "nodemon": "^3.1.9"
  },
  "devDependencies": {
    "axios-mock-adapter": "^2.1.0",
    "jest": "^29.7.0",
    "supertest": "^7.0.0"
  },
  "scripts": {
    "test": "jest --runInBand --verbose --silent"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setupTests.js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "node"
    ],
    "testMatch": [
      "**/?(*.)+(test).js"
    ]
  }
}
